.hljs
    display block
    overflow-x auto
    padding 0.5em
    color #383a42
    font-size 13px
    line-height 16px
    background #fafafa

.hljs-comment
.hljs-quote
.hljs-meta
    color #969896

.hljs-doctag
.hljs-keyword
.hljs-formula
    color #a71d5d

.hljs-name
.hljs-selector-tag
.hljs-deletion
.hljs-subst
    color #63a35c

.hljs-literal
    color #0184bb

.hljs-string
.hljs-regexp
.hljs-addition
.hljs-attribute
.hljs-meta-string
    color #183691

.hljs-number
.hljs-built_in
.hljs-class .hljs-title
    color #0086b3

.hljs-attr
.hljs-variable
.hljs-template-variable
.hljs-type
.hljs-selector-class
.hljs-selector-attr
.hljs-section
.hljs-title
.hljs-selector-pseudo
    color #795da3

.hljs-symbol
.hljs-bullet
.hljs-link
.hljs-selector-id
    color #4078f2

.hljs-emphasis
    font-style italic

.hljs-strong
    font-weight bold

.hljs-link
    text-decoration underline